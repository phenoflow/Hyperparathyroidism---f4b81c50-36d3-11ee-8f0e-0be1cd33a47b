      inputModule:
        id: inputModule
        source: inputModule4
      potentialCases:
        id: potentialCases
        source: hyperparathyroidism-cystica---primary/output
  hyperparathyroidism---secondary:
    run: hyperparathyroidism---secondary.cwl
